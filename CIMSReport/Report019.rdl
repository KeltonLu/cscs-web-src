  <DataSets>
    <DataSet Name="CIMS">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT     RPT_Report019.日期, TYPE, AFFINITY, PHOTO, RPT_Report019.版面簡稱, Name, Number,rowid
FROM         RPT_Report019  ,
     (SELECT     RPT_Report019.版面簡稱,  日期, ROW_NUMBER() OVER (ORDER BY 日期) rowid
FROM         RPT_Report019
GROUP BY 版面簡稱, 日期) AS table1 
where table1.版面簡稱 = RPT_Report019.版面簡稱  and table1.日期 = RPT_Report019.日期 and RPT_Report019.TimeMark =@TimeMark</CommandText>
        <QueryParameters>
          <QueryParameter Name="@TimeMark">
            <Value>=Parameters!TimeMark.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>CIMS</DataSourceName>
      </Query>
