      <Query>
        <DataSourceName>CIMS</DataSourceName>
        <CommandText>SELECT  wafer.Wafer_Name,
cancle.Stock_RID,
cancle.Perso_Factory_RID,
cancle.Cancel_Number,
CONVERT(varchar(12) , cancle.Cancel_Date, 111 ) as cancel_date,
cancle.Report_RID,
cancle.Comment,
type.Name,
blank.Factory_ShortName_CN as blankName,
perso.Factory_ShortName_CN as persoName
FROM DEPOSITORY_CANCEL cancle 
LEFT JOIN CARD_TYPE type ON  cancle.Space_Short_RID = type.RID
LEFT JOIN FACTORY blank ON cancle.Blank_Factory_RID = blank .RID
LEFT JOIN FACTORY perso ON cancle.Perso_Factory_RID = perso.RID
LEFT JOIN WAFER_INFO wafer ON cancle.Wafer_RID = wafer.RID
where cancle.RST = 'A' AND cancle.Report_RID = @report_rid</CommandText>
        <QueryParameters>
          <QueryParameter Name="@report_rid">
            <Value>=Parameters!report_rid.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
