      <Query>
        <DataSourceName>CIMS</DataSourceName>
        <CommandText>SELECT  Type, Stock_RID,  Space_Short_RID,Name,Factory_ShortName_CN, Blank_Factory_RID, Income_Number, Income_Date,  Comment, Real_Ask_Number, Unit_Price_No, Unit_Price, SAP_RID,  Is_Finance, Pay_Date, Real_Pay_Money, Real_Pay_Money_No,Is_AskFinance, Unit_Price_Order,Budget_ID,Agreement_Code  
FROM
 ( SELECT  '1' AS Type,  S.Stock_RID,  S.Space_Short_RID, S.Blank_Factory_RID, S.Income_Number, S.Income_Date,S.Is_AskFinance, D1.Comment, 
D1.Real_Ask_Number, D1.Unit_Price_No,
--case when D1.Real_Ask_Number is null then S.Income_Number else D1.Real_Ask_Number end as Real_Ask_Number, 
--case when D1.Unit_Price_No is null then O1.Unit_Price/1.05 else D1.Unit_Price_No end as Unit_Price_No, 
 D1.Unit_Price, D1.SAP_RID,  S1.Is_Finance, S1.Pay_Date, S1.Real_Pay_Money, S1.Real_Pay_Money_No, O1.Unit_Price AS Unit_Price_Order, 
C1.Name,F1. Factory_ShortName_CN
,CB.Budget_ID,AGM.Agreement_Code 
FROM DEPOSITORY_STOCK S  
LEFT JOIN CARD_TYPE C1  ON C1.RST = 'A'  AND C1.RID= S.Space_Short_RID  
INNER JOIN FACTORY F1 ON F1.RST = 'A'  AND F1.RID = S.Blank_Factory_RID and F1.Is_Blank='Y' AND F1.Is_Cooperate='Y'
 LEFT JOIN CARD_TYPE_SAP_DETAIL D1 ON  D1.Operate_RID = S.RID AND D1.Operate_Type = '1' AND D1.RST = 'A'  
LEFT JOIN CARD_TYPE_SAP S1 ON S1.RID=D1.SAP_RID  AND S1.RST = 'A'  
LEFT JOIN GROUP_CARD_TYPE G1 ON G1.CardType_RID=S.Space_Short_RID  AND G1.RST = 'A'  
LEFT JOIN ORDER_FORM_DETAIL O1 ON  O1.RST = 'A'  AND O1.OrderForm_Detail_RID=S.OrderForm_Detail_RID 
LEFT JOIN CARD_BUDGET CB ON CB.RST = 'A' AND S.Budget_RID = CB.RID
LEFT JOIN AGREEMENT AGM ON AGM.RST = 'A' AND S.Agreement_RID = AGM.RID
WHERE S.RST = 'A'  AND (S.Income_Date &lt;= @DateTo)    AND G1.Group_RID IN (@Group_RID )   AND  S.Blank_Factory_RID  IN (@RID)   
 AND (S1.Is_Finance='N'  OR S1.Is_Finance IS NULL OR( S1.Is_Finance='Y'  AND S1.Pay_Date&gt;@DateTo ))

 UNION 
SELECT   '2' AS Type,   R.Stock_RID, R.Space_Short_RID, R.Blank_Factory_RID, R.Reincome_Number AS Income_Number, R.Reincome_Date AS Income_Date,R.Is_AskFinance, D2.Comment, D2.Real_Ask_Number, D2.Unit_Price_No, D2.Unit_Price, D2.SAP_RID,  S2.Is_Finance, S2.Pay_Date, S2.Real_Pay_Money, S2.Real_Pay_Money_No, O2.Unit_Price AS Unit_Price_Order, 
C2.Name,F2. Factory_ShortName_CN
,CB.Budget_ID,AGM.Agreement_Code  
FROM DEPOSITORY_RESTOCK R    
LEFT JOIN CARD_TYPE C2  ON C2.RST = 'A'  AND C2.RID= R.Space_Short_RID  
INNER JOIN FACTORY F2 ON F2.RST = 'A'  AND F2.RID = R.Blank_Factory_RID and  F2.Is_Blank='Y' AND F2.Is_Cooperate='Y'
LEFT JOIN CARD_TYPE_SAP_DETAIL D2 ON  D2.Operate_RID = R.RID AND D2.Operate_Type = '2' AND D2.RST = 'A'   
LEFT JOIN CARD_TYPE_SAP S2 ON S2.RID=D2.SAP_RID  AND S2.RST = 'A'  
LEFT JOIN GROUP_CARD_TYPE G2 ON G2.CardType_RID=R.Space_Short_RID  AND G2.RST = 'A'   
LEFT JOIN ORDER_FORM_DETAIL O2 ON  O2.RST = 'A'  AND O2.OrderForm_Detail_RID=R.OrderForm_Detail_RID  
LEFT JOIN CARD_BUDGET CB ON CB.RST = 'A' AND R.Budget_RID = CB.RID
LEFT JOIN AGREEMENT AGM ON AGM.RST = 'A' AND R.Agreement_RID = AGM.RID
WHERE R.RST = 'A' AND (R.Reincome_Date &lt;= @DateTo)   AND G2.Group_RID IN (@Group_RID )    AND R.Blank_Factory_RID IN (@RID )     
AND (S2.Is_Finance='N' OR S2.Is_Finance IS NULL OR (S2.Is_Finance='Y' AND S2.Pay_Date&gt;@DateTo)) 
 UNION 
SELECT   '3' AS Type,  C.Stock_RID,  C.Space_Short_RID, C.Blank_Factory_RID, C.Cancel_Number AS Income_Number, C.Cancel_Date AS Income_Date, C.Is_AskFinance, D3.Comment, D3.Real_Ask_Number, D3.Unit_Price_No, D3.Unit_Price,  D3.SAP_RID,    S3.Is_Finance, S3.Pay_Date, S3.Real_Pay_Money, S3.Real_Pay_Money_No, O3.Unit_Price AS Unit_Price_Order, 
C3.Name,F3. Factory_ShortName_CN
,CB.Budget_ID,AGM.Agreement_Code   
FROM DEPOSITORY_CANCEL C 
LEFT JOIN CARD_TYPE C3  ON C3.RST = 'A'  AND C3.RID= C.Space_Short_RID  
INNER JOIN FACTORY F3 ON F3.RST = 'A'  AND F3.RID = C.Blank_Factory_RID  and  F3.Is_Blank='Y' AND F3.Is_Cooperate='Y'
LEFT JOIN CARD_TYPE_SAP_DETAIL D3 ON  D3.Operate_RID = C.RID AND D3.Operate_Type = '3' AND D3.RST = 'A'   
LEFT JOIN CARD_TYPE_SAP S3 ON S3.RID=D3.SAP_RID  AND S3.RST = 'A' 
LEFT JOIN GROUP_CARD_TYPE G3 ON G3.CardType_RID=C.Space_Short_RID AND G3.RST = 'A'   
LEFT JOIN ORDER_FORM_DETAIL O3 ON  O3.RST = 'A'  AND  O3.OrderForm_Detail_RID=C.OrderForm_Detail_RID
LEFT JOIN CARD_BUDGET CB ON CB.RST = 'A' AND C.Budget_RID = CB.RID
LEFT JOIN AGREEMENT AGM ON AGM.RST = 'A' AND C.Agreement_RID = AGM.RID 
WHERE C.RST = 'A' AND (C.Cancel_Date &lt;= @DateTo)    AND G3.Group_RID IN (@Group_RID )   AND C.Blank_Factory_RID IN (@RID )  
AND (S3.Is_Finance='N' OR S3.Is_Finance IS NULL OR (S3.Is_Finance='Y' AND S3.Pay_Date&gt;@DateTo)) 
) 
as table1 where Convert(datetime,substring(stock_rid,1,8))&gt;='2008-09-01'   and Unit_Price_Order is not null and Budget_ID &lt;&gt; '舊系統預算' and Agreement_Code &lt;&gt; '舊系統合約'
order by Income_Date</CommandText>
        <QueryParameters>
          <QueryParameter Name="@DateTo">
            <Value>=Parameters!DateTo.Value</Value>
