  <rd:ReportID>7fff935e-e580-4516-b0cb-94a3d3a87099</rd:ReportID>
  <LeftMargin>1cm</LeftMargin>
  <DataSets>
    <DataSet Name="CIMS">
      <Query>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <CommandText>SELECT F_P .Factory_ShortName_CN AS PersoFactoryName,Stock_RID,CT.NAME,Income_Number,Income_Date,WI.WAFER_NAME,F_B.Factory_ShortName_CN,DS.Comment,ds.report_rid,ds.rid
FROM DEPOSITORY_STOCK DS
LEFT join FACTORY F_P ON F_P.RST = 'A' AND F_P.IS_Perso ='Y' and F_P.RID=DS.Perso_Factory_RID
LEFT join CARD_TYPE CT ON CT.RST='A' AND CT.RID=DS.Space_Short_RID 
LEFT JOIN WAFER_INFO WI ON WI.RST='A' AND WI.RID=DS.Wafer_RID 
LEFT join FACTORY F_B ON F_B.RST = 'A' AND F_B.IS_BLANK ='Y' AND F_B.RID=DS.Blank_Factory_RID  
WHERE Report_RID in (select Report_RID from DEPOSITORY_STOCK where RID=@RID)</CommandText>
        <QueryParameters>
          <QueryParameter Name="@RID">
            <Value>=Parameters!RID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <DataSourceName>CIMS</DataSourceName>
      </Query>
